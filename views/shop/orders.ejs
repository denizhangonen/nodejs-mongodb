<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length <= 0) { %>
                <h1>Nothing there!</h1>
            <% } else { %>
                <ul>
                    <% orders.forEach(order => { %>
                        <li># <%=order._id %></li>
                        <ul>
                            <% order.items.forEach(product => { %>
                                <li><%= product.title %> (<%= product.quantity %>)</li>
                            <% }) %>
                        </ul>
                    <% }) %>    
                </ul>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>